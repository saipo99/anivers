<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/BotonDeslis"
        android:layout_width="85dp"
        android:layout_height="72dp"
        android:text="@string/deslisa"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/ImgBotonAnime"
        android:layout_width="161dp"
        android:layout_height="224dp"
        app:srcCompat="@drawable/maxresdefault"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="132dp" />

    <ImageButton
        android:id="@+id/ImgBotonConosenos"
        android:layout_width="160dp"
        android:layout_height="223dp"
        app:srcCompat="@drawable/48b395c55ecc2b8bc4fc0812dec3a30e"
        tools:layout_editor_absoluteX="218dp"
        tools:layout_editor_absoluteY="383dp" />

    <ImageButton
        android:id="@+id/ImgButMusica"
        android:layout_width="162dp"
        android:layout_height="222dp"
        android:foreground="@drawable/img_1888"
